{
  "extends": "@vue/tsconfig/tsconfig.node.json",
  "include": [
    "types/module.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "vite.config.ts",
    "vite/**/*.ts",
    "mock/**/*.ts",
    "types/**/*.d.ts",
    "types/**/*.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "composite": true,
    "types": ["node"]
  }
}
